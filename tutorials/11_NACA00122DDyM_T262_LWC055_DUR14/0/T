FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../caseConditions"

dimensions      [0 0 0 1 0 0 0];

internalField   uniform $Temperature;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform $Temperature;
    }
    outlet
    {
        type            zeroGradient;
    }
    region0_to_wallFilmRegion_wallFilmFaces
    {
        type            fixedValue;
        value           uniform $Temperature;
    }

    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}

